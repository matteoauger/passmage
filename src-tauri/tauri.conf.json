{
    "$schema": "https://schema.tauri.app/config/2.0.0-rc",
    "productName": "passmage",
    "version": "1.0.0",
    "identifier": "com.passmage.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "passmage",
                "width": 800,
                "height": 616,
                "minWidth": 800,
                "minHeight": 616
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}
